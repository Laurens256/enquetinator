<header>
	<div>
		<h1>Overzicht</h1>
		<p>Controleer je antwoorden voor het inleveren</p>
	</div>
</header>

<main>
	<article>
		<ul>
			{{#each completeSubjects}}
				<li>
					<a class="{{#if this}}completed{{else}}uncompleted{{/if}}" href='/enquete?vak={{@key}}'>{{subjectName @key}}
						<div class="{{#if this}}checkmark{{else}}cross{{/if}} checkmark"></div>
					</a>
				</li>
			{{/each}}
		</ul>
	</article>
	<a href="#" class="underline">Verzenden</a>
</main>